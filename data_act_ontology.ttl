dataact:actsOnBehalfOf rdf:type owl:ObjectProperty ;
                       rdfs:domain dataact:DataIntermediationService ;
                       rdfs:range dataact:User ;
                       rdfs:comment "Recitals 29-33 - Intermediary may exercise user rights" ;
                       rdfs:label "acts on behalf of" .


