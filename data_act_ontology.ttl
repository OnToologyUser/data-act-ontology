@base <https://w3id.org/def/dataact#> .

<https://w3id.org/def/dataact> rdf:type owl:Ontology ;
                                ns:license <http://creativecommons.org/licenses/by-sa/4.0/> ;
                                terms:license <http://creativecommons.org/licenses/by-sa/4.0/> ;
                                rdfs:comment "Comprehensive Ontology for the EU Data Act (Regulation 2023/2854)" ;
                                rdfs:label "EU Data Act Ontology" ;
                                owl:versionInfo "2.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
ns:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#member
skos:member rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:AnnotationProperty .


###  https://w3id.org/def/dataact#legalEffect
dataact:legalEffect rdf:type owl:AnnotationProperty ;
                    rdfs:comment "Legal effect description" ;
                    rdfs:label "legal effect" .


###  https://w3id.org/def/dataact#legalSource
dataact:legalSource rdf:type owl:AnnotationProperty ;
                    rdfs:comment "Reference to legal document or provision" ;
                    rdfs:label "legal source" .


###  https://w3id.org/def/dataact#qualifiedBy
dataact:qualifiedBy rdf:type owl:AnnotationProperty ;
                    rdfs:comment "Qualification or condition" ;
                    rdfs:label "qualified by" .


###  https://w3id.org/def/dataact#ruleSource
dataact:ruleSource rdf:type owl:AnnotationProperty ;
                   rdfs:comment "Reference to specific article or recital" ;
                   rdfs:label "rule source" .


###  https://w3id.org/def/dataact#temporalExtent
dataact:temporalExtent rdf:type owl:AnnotationProperty ;
                       rdfs:comment "Temporal period during which something holds" ;
                       rdfs:label "temporal extent" .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.estrellaproject.org/lkif-core/expression.owl#qualifies
<http://www.estrellaproject.org/lkif-core/expression.owl#qualifies> rdf:type owl:ObjectProperty ;
                                                                    rdfs:domain dataact:Qualification ;
                                                                    rdfs:range <http://www.estrellaproject.org/lkif-core/norm.owl#Right> ;
                                                                    rdfs:comment "LKIF: Adds qualification to norm or right" ;
                                                                    rdfs:label "qualifies" .


###  http://www.estrellaproject.org/lkif-core/role.owl#plays
<http://www.estrellaproject.org/lkif-core/role.owl#plays> rdf:type owl:ObjectProperty ;
                                                          rdfs:domain odrl:Party ;
                                                          rdfs:range dataact:LegalRelationship ;
                                                          rdfs:comment "Specifies that some thing plays a role" ;
                                                          rdfs:label "plays role in" .


###  https://w3c.github.io/dpv/2.2/dpv/#hasData
dpv:hasData rdf:type owl:ObjectProperty ;
            rdfs:domain dataact:DataHolder ;
            rdfs:range dataact:Data ;
            rdfs:comment "Article 2(13) - Data holder has rights to control access to data" ;
            rdfs:label "has data" .


###  https://w3c.github.io/dpv/2.2/dpv/#hasDataSubject
dpv:hasDataSubject rdf:type owl:ObjectProperty ;
                   owl:inverseOf dataact:isSubjectOf ;
                   rdfs:domain dpv:PersonalData ;
                   rdfs:range dpv:DataSubject ;
                   rdfs:comment "Indicates association with Data Subject" ;
                   rdfs:label "has data subject" .


###  https://w3c.github.io/dpv/2.2/dpv/#hasPurpose
dpv:hasPurpose rdf:type owl:ObjectProperty ;
               rdfs:domain odrl:Party ;
               rdfs:range odrl:purpose ;
               rdfs:comment "Indicates association with Purpose" ;
               rdfs:label "has purpose" .


###  https://w3c.github.io/dpv/2.2/dpv/#hasRecipient
dpv:hasRecipient rdf:type owl:ObjectProperty ;
                 owl:inverseOf dataact:receivesDataFrom ;
                 rdfs:domain dataact:DataHolder ;
                 rdfs:range odrl:Party ;
                 rdfs:comment "Article 5 and Article 8 - Data provision to recipients" ;
                 rdfs:label "has recipient" ;
                 skos:altLabel "provides data to" .


