dataact:authorizedBy rdf:type owl:ObjectProperty ;
                     rdfs:domain dataact:DataSharing ;
                     rdfs:range dataact:User ;
                     rdfs:comment "Article 8(4) - B2B data sharing must be authorized by the user" ;
                     rdfs:label "authorized by" .


###  https://w3id.org/def/dataact#classifiedAs
dataact:classifiedAs rdf:type owl:ObjectProperty ;
                     rdfs:domain dataact:Data ;
                     rdfs:range dataact:DataCategory ;
                     rdfs:label "classified as" .


