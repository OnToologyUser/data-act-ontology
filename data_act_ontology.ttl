dataact:compliesWith rdf:type owl:ObjectProperty ;
                     rdfs:domain dataact:DataProcessingService ;
                     rdfs:range dataact:InteroperabilityStandard ;
                     rdfs:comment "Article 34-35 - Interoperability compliance" ;
                     rdfs:label "complies with" .


###  https://w3id.org/def/dataact#createsDuty
dataact:createsDuty rdf:type owl:ObjectProperty ;
                    rdfs:domain dataact:LegalAction ;
                    rdfs:range odrl:Duty ;
                    rdfs:label "creates duty" .


